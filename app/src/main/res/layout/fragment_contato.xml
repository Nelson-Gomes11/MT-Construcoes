<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".ui.contato.ContatoFragment"
    android:background="@color/brown">

    <ImageView
        android:id="@+id/logo"
        android:layout_width="334dp"
        android:layout_height="211dp"
        android:layout_marginTop="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginLeft="40dp"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logo_mt" />

    <EditText
        android:id="@+id/contatoemp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="235dp"
        android:fontFamily="@font/inter_black"
        android:text="@string/informacoes_de_contato"
        android:textSize="25dp"
        app:layout_constraintTop_toBottomOf="@id/logo" />

    <EditText
        android:id="@+id/emailcont"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="285dp"
        android:fontFamily="@font/inter_bold"
        android:text="@string/Email"
        android:textSize="20dp"
        app:layout_constraintTop_toBottomOf="@id/contatoemp"/>
    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="350dp"
        android:layout_marginLeft="20dp"
        android:fontFamily="@font/inter_bold"
        android:text="@string/Telefone"
        android:textSize="20dp"
        app:layout_constraintTop_toBottomOf="@id/emailcont"/>




</FrameLayout>